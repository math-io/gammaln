{
  "name": "math-gammaln",
  "version": "0.0.0",
  "description": "Natural logarithm of the gamma function.",
  "author": {
    "name": "Philipp Burckhardt",
    "email": "pburckhardt@outlook.com"
  },
  "contributors": [
    {
      "name": "Philipp Burckhardt",
      "email": "pburckhardt@outlook.com"
    }
  ],
  "scripts": {
    "test": "if [ \"${TRAVIS}\" ]; then npm run test-ci; else npm run test-local; fi",
    "test-local": "tape \"./test/*.js\" | tap-spec",
    "test-ci": "npm run test-local && xvfb-run npm run test-browsers",
    "test-cov": "istanbul cover --dir ./reports/coverage --report lcov tape -- \"./test/*.js\"",
    "test-browsers": "browserify ./test/*.js | testling | tap-spec",
    "coverage": "istanbul cover --dir ./reports/codecov/coverage --report lcovonly tape -- \"./test/*.js\" && cat ./reports/codecov/coverage/lcov.info | codecov && rm -rf ./reports/codecov"
  },
  "main": "./lib",
  "repository": {
    "type": "git",
    "url": "git://github.com/math-io/gammaln.git"
  },
  "keywords": [
    "compute.io",
    "compute-io",
    "compute",
    "computation",
    "math.io",
    "math-io",
    "math",
    "mathematics",
    "special function",
    "gamma",
    "factorial",
    "log-scale",
    "logarithm",
    "log",
    "ln",
    "natural",
    "scalar",
    "number"
  ],
  "bugs": {
    "url": "https://github.com/math-io/gammaln/issues"
  },
  "dependencies": {
    "const-pi": "^1.0.3",
    "const-pinf-float64": "^1.0.0",
    "math-abs": "^1.0.2",
    "math-cos": "^1.0.0",
    "math-floor": "^1.0.1",
    "math-ln": "^1.0.0",
    "math-power": "^1.0.0",
    "math-sin": "^1.0.0",
    "math-trunc": "^1.0.0",
    "validate.io-finite": "^1.0.0",
    "validate.io-nan": "^1.0.3"
  },
  "devDependencies": {
    "browserify": "12.x.x",
    "codecov": "1.x.x",
    "compute-linspace": "^1.0.0",
    "istanbul": "^0.4.1",
    "jshint": "2.x.x",
    "jshint-stylish": "2.x.x",
    "tap-spec": "4.x.x",
    "tape": "4.x.x",
    "testling": "1.x.x"
  },
  "testling": {
    "files": [
      "test/*.js"
    ],
    "browsers": [
      "iexplore/6.0..latest",
      "firefox/3.0..latest",
      "firefox/nightly",
      "chrome/4.0..latest",
      "chrome/canary",
      "opera/10.0..latest",
      "opera/next",
      "safari/4.0..latest",
      "ipad/6.0..latest",
      "iphone/6.0..latest",
      "android-browser/4.2..latest"
    ]
  },
  "license": "MIT"
}
